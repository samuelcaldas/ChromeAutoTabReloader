{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.1",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "__MSG_extensionName__"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en"
}